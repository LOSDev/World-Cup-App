{{#each matches}}

 

  <div class="panel panel-success match-table match-view" data-id="{{ id }}">
    <a href="#">
    <div class="panel-heading">
      <span>{{match_type}} {{#if group }}Group {{/if}}{{ group }}</span></th>      
               
      <span class="badge pull-right">{{startDay}}.{{startMonth}}.{{ startYear }} at {{startHours}}:{{startMinutes}}</span>
    </div>
    <table class="table">
      
      
      <tbody>
        <tr>
          <td width="10%" align="right"><span>{{{ home_flag }}}</span></td>

          <td width="25%" align="right"><span>{{ home_team.name }}</span></td>
          <td width="10%" align="center"><span>{{ home_goals }}</span></td>
          <td width="10%" align="center"><span>-</span></td>
          <td width="10%" align="center"><span>{{ away_goals}}</span></td>
          <td width="25%" align="left"><span>{{ away_team.name }}</span></td>
          <td width="10%" align="right"><span>{{{ away_flag }}}</span></td>

        </tr>
      </tbody>
      
    </table>
    </a>
    <div class="match-details">
      
    </div>
  </div>
{{/each}}